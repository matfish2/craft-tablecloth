<template x-if="{{ value }}.length>0">
    <template x-for="(entry,i) in {{ value }}">
        <span>
        <a :href="entry.data.url" x-text="entry.data.title" target="_blank">
        </a><template x-if="i<{{ value }}.length-1"><span>,</span></template>
        </span>
    </template>
</template>
